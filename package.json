{
    "name": "coinBot",
    "version": "2.0.0",
    "description": "A farming bot",
    "main": "nodemon index.js",
    "scripts": {
        "test": "echo \"honk honk\" && exit 1",
        "update": "git pull --rebase origin master",
        "start": "nodemon index.js 0 ",
        "start1": "nodemon index.js 1 ",
        "start2": "nodemon index.js 2",
        "dev": "concurrently \"npm run start1\" \"npm run start2\" \"npm run start\"",
        "pm2": "pm2 -f --restart-delay=10 start index.js -- 0 && pm2 -f --restart-delay=10 start index.js -- 1 && pm2 -f --restart-delay=10 start index.js -- 2 && pm2 flush && pm2 logs",
        "dpm2": "pm2 delete all"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "concurrently": "^5.3.0",
        "console-stamp": "^3.0.0-rc4.2",
        "discord.js": "^12.1.1",
        "discordie": "^0.11.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.6"
    }
}
